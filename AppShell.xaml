<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="DistributeurATM.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:DistributeurATM"
    xmlns:view="clr-namespace:DistributeurATM.View"
    Shell.FlyoutBehavior="Disabled"
    Title="DistributeurATM">
    <TabBar>
        <ShellContent
         Title="dépôtpage"
         Route="depotpage"
         ContentTemplate="{DataTemplate view:dépôtpage}" />

        <ShellContent
         Title="bossPage"
         Route="bossPage"
         ContentTemplate="{DataTemplate view:bossPage}" />
    </TabBar>
</Shell>
